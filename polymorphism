class Vehicles:
    def __init__(self, action):
        self.action = action
    def move(self)->None:
        raise NotImplementedError("Subclasses must implement this method.")
    
class Car(Vehicles):
    def move(self)->None:
        return f"{self.action}"
    
class Plane(Vehicles):
    def move(self)->None:
        return f"{self.action}"
    
